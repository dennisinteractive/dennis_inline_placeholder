<?php
/**
 * @file
 * dennis_inline_placeholder_wysiwyg.module
 *
 * Contains WYSIWYG features to insert ad slots inline into articles.
 */

/**
 * Implements hook_wysiwyg_include_directory().
 */
function dennis_inline_placeholder_wysiwyg_wysiwyg_include_directory($type) {
  switch ($type) {
    case 'plugins':
      return 'plugins/wysiwyg';
  }
}
